  "description": "A Genetic Algorithm library designed for browser and node.js",
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-typescript": "^7.6.0",
    "@types/chai": "^4.2.5",
    "@types/eyes": "^0.1.31",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.8",
    "babel-loader": "^8.0.6",
    "chai": "^4.2.0",
    "concurrently": "^4.1.2",
    "cross-env": "^6.0.3",
    "eyes": "^0.1.8",
    "fork-ts-checker-webpack-plugin": "^1.5.0",
    "mocha": "^6.2.2",
    "mocha-typescript": "^1.1.17",
    "nyc": "^14.1.1",
    "source-map-support": "^0.5.16",
    "ts-node": "^8.5.2",
    "tslint": "^5.20.0",
    "typescript": "^3.7.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9",
    "webpack-closure-compiler": "^2.1.6"
  },
  "files": [
    "lib/**/*",
    "lib-esm/**/*",
    "_bundles/**/*"
  ],
  "homepage": "https://github.com/mmmaxou/genetical",
  "keywords": [
    "genetical",
    "genetic",
    "algorithm",
    "genetic algorithm",
    "ag",
    "evolution",
    "fitness",
    "machine",
    "learning",
    "machine learning"
  ],
  "license": "ISC",
  "main": "lib/src/genetical.js",
  "name": "genetical",
  "repository": {
    "type": "git",
    "url": "git://github.com/mmmaxou/genetical.git"
  },
  "types": "lib/types/src/genetical.d.ts",
  "scripts": {
    "clean": "shx rm -rf _bundles lib lib-esm dist",
    "pretest": "tsc && tsc -m es6 --outDir lib-esm",
    "prepare": "tsc && tsc -m es6 --outDir lib-esm",
    "serve": "webpack --watch --mode=development",
    "nodemon": "nodemon dist/genetical.js",
    "build": "cross-env NODE_ENV=production npm run clean && npm run build:types && npm run build:js && npm run build:prod",
    "build:js": "cross-env NODE_ENV=production tsc && tsc -m es6 --outDir lib-esm",
    "build:types": "cross-env NODE_ENV=production tsc --emitDeclarationOnly",
    "build:prod": "cross-env NODE_ENV=production webpack --mode=production",
    "dev": "concurrently -k -n \"Typescript,Node\" -p \"[{name}]\" -c \"blue,green\" \"npm:serve\" \"npm:nodemon\"",
    "start": "tsc && tsc -m es6 --outDir lib-esm && node dist/genetical.js",
    "test": "mocha",
    "test:watch": "mocha --watch",
    "coverage": "nyc npm run test"
  },
  "nyc": {
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "version": "1.0.0"
}
